set appname     eagle
set version     2.4.1
set prefix      /export/apps/${appname}/${version}
set url         "https://alkesgroup.broadinstitute.org/Eagle"
set msg         "Haplotype phase estimation either within a genotyped cohort or using a phased reference panel.\nOfficial site: $url\n"

proc ModulesHelp{ } {
    puts stderr $msg
}

module-whatis   $msg

conflict        $appname

prepend-path    PATH        ${prefix}

