set     appname         cytoscape
set     version         3.10.2
set     prefix          /export/apps/${appname}/${version}
set     exec_prefix     ${prefix}

conflict    cytoscape

prepend-path    PATH            ${exec_prefix}

# Requires java/17 since we only have Java 11 in CentOS 8
if { ![is-loaded java/17] } {
	module load java/17
}
