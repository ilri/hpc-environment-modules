set appname     minimac3
# built from git commit: f14281027e9b1686e22f50501d2f7751e47724d9
set version     2.0.1
set prefix      /export/apps/${appname}/${version}
set exec_prefix ${prefix}/bin
set url         "https://github.com/Santy-8128/Minimac3"
set msg         "Minimac3 is a lower memory and more computationally efficient implementation of the genotype imputation algorithms in minimac and minimac2.\nOfficial site: $url\n"

proc ModulesHelp{ } {
    puts stderr $msg
}

module-whatis   $msg

conflict        $appname

prepend-path    PATH        ${exec_prefix}

